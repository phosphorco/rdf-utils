{
  "name": "@phosphorco/rdf-utils",
  "version": "0.1.0",
  "description": "A TypeScript library for RDF utilities and resource management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bun run clean && bun tsc && bun build src/index.ts --outdir=dist --target=node",
    "test": "bun test test/",
    "test:watch": "bun test test/ --watch",
    "test:integration": "bun test test-integration/",
    "test:all": "bun test test/ test-integration/",
    "dev": "bun --watch src/index.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "typescript",
    "library",
    "rdf",
    "utils",
    "phosphorco"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.2.18",
    "@types/rdfjs__data-model": "^2.0.9",
    "@types/rdfjs__dataset": "^2.0.7",
    "@types/sparqljs": "^3.1.10",
    "fast-check": "^4.2.0",
    "n3": "^1.26.0",
    "stardog": "^8.0.1",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md"
  ],
  "dependencies": {
    "@comunica/query-sparql": "^3.2.3",
    "@rdfjs/data-model": "^2.1.1",
    "@rdfjs/dataset": "^2.0.2",
    "@rdfjs/namespace": "^2.0.1",
    "@rdfjs/types": "^2.0.1",
    "@types/n3": "^1.26.0",
    "immutable": "^5.1.3",
    "sparqljs": "^3.7.3"
  }
}
